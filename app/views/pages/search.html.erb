<h2>All Users</h2>

<ul>
  <% @users.each do |user| %>
    <li>
      <a href="<%= user_url(user) %>"><%= user.name %> </a>
      @<%= user.username %>
    </li>
  <% end %>
</ul>

<h2>All Hashtags</h2>

<ul>
  <% @hashtags.each do |hashtag| %>
    <li>
      <a href="<%= hashtag_url(hashtag) %>">#<%= hashtag.name %></a>
    </li>
  <% end %>
</ul>