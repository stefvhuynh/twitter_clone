<h2>
  <a href='<%= user_url(current_user) %>'>
    <%= current_user.name %>
  </a>
</h2>

<small>
  <a href='<%= user_url(current_user) %>'>
    @<%= current_user.username %>
  </a>
</small>

<h1>Tweets</h1>

<ul>
  <% @tweets.each do |tweet| %>
    <li><%= render partial: 'tweets/preview', locals: { tweet: tweet } %></li>
  <% end %>
</ul>